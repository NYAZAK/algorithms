{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
      "include": [
          "./src/"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "docs"
  },
  "plugins": [],
  "opts": {
      "template": "node_modules/minami",
      "encoding": "utf8",
      "destination": "docs",
      "recurse": true,
      "private": false,
      "readme": "./readme.md"
  }
}